platform :ios, '9.0'
swift_version = '3.0'

abstract_target :Common do
  use_frameworks!

  target :Plain do
    pod 'Firebase/Core'
    pod 'Firebase/AdMob'
    pod 'SwiftEssential', :git => 'https://github.com/ntaku/SwiftEssential.git'
    pod 'SwiftDate'
    pod 'R.swift'
    pod 'SDWebImage'
    pod 'Alamofire'
    pod 'LUKeychainAccess'
  end
end
